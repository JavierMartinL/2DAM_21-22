<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="es.system.javier.ong">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Ong_Javier">
        <activity
            android:name="es.system.javier.ong.activity.DetailMascotaActivity"
            android:label="Datos Mascota"
            android:parentActivityName="es.system.javier.ong.activity.MainActivity"
            android:exported="false">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="es.system.javier.ong.activity.MainActivity"/>
        </activity>
        <activity
            android:name="es.system.javier.ong.activity.AddEditMascotaActivity"
            android:label="Datos Persona"
            android:parentActivityName="es.system.javier.ong.activity.MainActivity"
            android:exported="false" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="es.system.javier.ong.activity.MainActivity"/>
        </activity>
        <activity
            android:name="es.system.javier.ong.activity.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>